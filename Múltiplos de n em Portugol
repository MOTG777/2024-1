programa {
  funcao inicio() {

    inteiro n, li, ls

    escreva("VocÃª quer os mÃºltiplos de que nÃºmero? ")
    leia(n)

    escreva("Qual o limite inferior? ")
    leia(li)

    escreva("Qual o limite superior? ")
    leia(ls)

    enquanto(ls>=li){
      se(li % n == 0)
      escreva(li, "\n")
      li++
    
    }
    
  }
}
